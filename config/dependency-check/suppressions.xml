<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- Suppress false positives for Spring Boot dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress false positive for Spring Boot starter dependencies
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring-boot-starter.*$</packageUrl>
        <vulnerabilityName>CVE-2016-1000027</vulnerabilityName>
    </suppress>
    
    <!-- Suppress false positives for Jackson dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress false positive for Jackson JSON processing library
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/com\.fasterxml\.jackson\.core/.*$</packageUrl>
        <vulnerabilityName>CVE-2020-25649</vulnerabilityName>
    </suppress>
    
    <!-- Suppress false positives for Netty dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress false positive for Netty networking framework
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.netty/.*$</packageUrl>
        <vulnerabilityName>CVE-2021-21290</vulnerabilityName>
    </suppress>
    
    <!-- Suppress false positives for Reactor dependencies -->
    <suppress>
        <notes><![CDATA[
        Suppress false positive for Project Reactor
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/io\.projectreactor/.*$</packageUrl>
        <vulnerabilityName>CVE-2021-21290</vulnerabilityName>
    </suppress>
</suppressions>
